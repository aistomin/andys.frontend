{
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "transform": {
    "^.+\\.[t|j]sx?$": "babel-jest",
    ".+\\.(css|less|sass|scss|png|jpg|gif|ttf|woff|woff2|svg)$": "jest-transform-stub"
  },
  "moduleNameMapper": {
    "^.+.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
  },
  "setupFiles": [
    "jest-canvas-mock"
  ],
  "setupFilesAfterEnv": [
    "./src/__setup__/setupTests.jsx"
  ],
  "testEnvironment": "jsdom"
}
